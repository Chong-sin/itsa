#include <iostream>
#include <iomanip> // 需要包含此標頭檔來使用 setprecision

using namespace std;

int main() {
    int miles;
    double kilometers;
    
    cin >> miles; // 使用 >> 而不是 << 來讀取輸入

    kilometers = miles * 1.6;
    cout << fixed << setprecision(1) << kilometers << endl; // 使用 kilometers 而不是 input

    return 0;
}
