#include <iostream>
using namespace std;


int main(){
    int a, b, c, d, time;
    cin>>a>>b>>c>>d;
    time = (c - a) * 60 + (d - b);
    if(time < 120)    cout<<time / 30 * 30<<endl;
    else if(time < 240)   cout<<(time - 120) / 30 * 40 + 120<<endl;
    else    cout<<(time - 240) / 30 * 60 + 280<<endl;
    return 0;
}
