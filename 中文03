#include <stdio.h>    
    
int main(void){    
  int arr[100];    
  int i = 0;    
  int a;  
  int n = 6;  
  int sum = 0;  
  while(n--){    
    scanf("%d",&a);  
    arr[i++] = a * a * a;  
    sum += arr[i - 1];  
  }    
  int j;    
  // for(j = i-1; j >= 0; j--){    
  //   if(j == i-1)    printf("%d",arr[j]);    
  //   else    printf(" %d",arr[j]);    
  // }    
  printf("%d",sum);  
  printf("\n");    
  return 0;    
}  
